function vowelsAndConsonants(s) {
    var len = s.length; 
    for(var i = 0 ; i < len; i++){
        if (s[i] == 'a' || s[i] == 'e' ||s[i] == 'i' ||s[i] == 'o'|| s[i] == 'u' ){
            
          console.log(s[i]) ;
          
        } };
    
       for(var i = 0 ; i < len; i++){
        if (s[i] != 'a' && s[i] != 'e' && s[i] != 'i' && s[i] != 'o' && s[i] != 'u' ){
            
          console.log(s[i]) ;
        }; 
        
    }
 
};
